spring:
  main:
    allow-bean-definition-overriding: true
  cloud:
    alibaba:
      seata:
        tx-service-group: project_tx_group
seata:
  enabled: true
  enable-auto-data-source-proxy: true
  tx-service-group: project_tx_group
  registry:
    type: nacos
    nacos:
      server-addr: ${spring.profiles.ip}:8848
      username: nacos
      password: nacos
      namespace: seata
  config:
    type: nacos
    nacos:
      server-addr: ${spring.profiles.ip}:8848
      group: SEATA_GROUP
      username: nacos
      password: nacos
      namespace: seata
  service:
    vgroup-mapping:
      project_tx_group: default
    disable-global-transaction: false
  client:
    rm:
      report-success-enable: false